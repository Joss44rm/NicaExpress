<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:NicaExpress.Vistas"
            x:Class="NicaExpress.Nicaexpress">
    <FlyoutPage.Flyout>
        <local:FlyoutMenuPage x:Name="flyoutpage"></local:FlyoutMenuPage>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:Reservacion></local:Reservacion>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>